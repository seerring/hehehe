  z-index: 10;
  padding-bottom: 30rpx;
}
/* 地图小图标 */
.mapIcon{
  height: 88rpx;
  line-height: 88rpx;
  display: flex;
  align-items: center;
}

/* vip图片样式 */
.bg{
  width: 690rpx;
