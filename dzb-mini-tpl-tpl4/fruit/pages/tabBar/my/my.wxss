     border-top-left-radius: 8rpx;
     border-top-right-radius: 8rpx;
}
.allOrder image{
     width:11rpx;
     height:22rpx;
}
/* 核销订单 */
.cancelOrder{
    additive-symbols:1rpx solid #eeeeee;
    margin-top: 12rpx;
    border: none;
    border-radius: 8rpx;
}
.hxNavigator{
  width: 100%;
  height: 100rpx;
}
.orderInfo{
     padding:32rpx 0 38rpx 0;
