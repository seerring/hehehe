.container{
  background:  -webkit-linear-gradient(top,#1ccfcf,#fff);
}
.topInfo{
     width:690rpx;
     position: relative;
     background: #fff;
     border-radius: 24rpx;
}
/* 头像的背景 */
.headBg{
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 164rpx 164rpx;
}
/* 附近店铺标题样式 */
.shopTitle{
  width: 690rpx;
