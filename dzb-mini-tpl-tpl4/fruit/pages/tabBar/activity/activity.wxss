/* pages/tabBar/activity/activity.wxss */
.group {
  min-height: calc(99.12vh);
}
.group /deep/ .vux-header {
  border-bottom: 0px;
}
.group .groupBox {
  /*height:calc(100vh - 1.86rem);
            overflow-y:scroll;*/
  padding-bottom: 98rpx;
}
.group .groupBox .storeInfo {
  width: 100%;
  height: 375rpx;

}
/* 页面背景色 */
page{
  background:#f4f4f4;
}
/* 头部背景图 */
.topBg{
  height: 375rpx;
  width: 100%;
  z-index: -1;
}
/* 店面标题与头像 */
.storeTitle{
  width: 690rpx;
  height: 130rpx;
  background: rgba(255, 255, 255, 0.8);
  border: 0.5px solid #fff;
  border-radius: 12rpx;
}
.storeImg{
  width: 90rpx;
  height: 90rpx;
  margin-left: 30rpx;
  /* margin-top: 30rpx; */
  overflow: hidden;
  /* border: 1px solid black; */
}
.storeImg image{
  width: 100%;
  height: 100%;
}
.storeText{
  margin-left: 30rpx;
}
.distance{
  right:20rpx;
}
/* 限时特价 限时拼团 */
.speGroupTab{
  display: flex;
  flex-direction: row;
}

/* .special, .getGroup{
  
} */
.getGroup{
  margin-left: 62rpx;
}
.group .groupBox .storeInfo .storeBox {
  font-family: 'PingFang-Medium';
  margin-top: 215rpx;
}
.group .groupBox .storeInfo .storeBox .leftCont {
  width: 26%;
  padding-left: 50rpx;
}
.group .groupBox .storeInfo .storeBox .leftCont image {
  width: 100rpx;
  height: 100rpx;
}
.group .groupBox .storeInfo .storeBox .rightCont {
  width: 74%;
  height: 80rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.group .groupBox .storeInfo .storeBox .rightCont .address view {
  width: 8%;
}
.group .groupBox .storeInfo .storeBox .rightCont .address view image {
  width: 30rpx;
  height: 38rpx;
}
.group .groupBox .storeInfo .storeBox .rightCont .address view {
  width: 92%;
}
.group .groupBox .tabList {
  font-family: 'PingFang-Medium';
}
.group .groupBox .tabList text {
  display: inline-block;
  text-align:center;
}
/* 共同的样式 */
.sameTab{
  display: block;
  width: 330rpx;
  height: 120rpx;
  border-radius: 12rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  background-repeat: no-repeat;
  background-size: 330rpx 120rpx;
}
/* 选中的样式 */
.selected {
  background-color: rgba(28, 207, 207, 0.2);
  color: rgb(28, 207, 207);
  /* box-shadow: 0rpx 10rpx 40rpx 0 rgba(110,110,110,0.1); */
  box-shadow: 0rpx 6rpx 16rpx 0rpx rgba(110, 110, 110, 0.1);
}
/* 未选中的样式 */
.notSelected{
  background-color: rgba(102, 102, 102, 0.2);
  color: #666;
  }
/* 单独的四个样式控制tab的背景图片 */
.tjSelect{
  background-image: url(data:image/png;base64,);
}
.tjNSelect{
  background-image: url(data:image/png;base64,);
}
.ptNSelect{
  background-image: url(data:image/png;base64,);
}
.ptSelect{
  background-image: url(data:image/png;base64,);
}

.group .groupBox .tabList .rentOut,
.group .groupBox .tabList .sell {
  width: 40%;
}
.tabList{
  margin-top: 30rpx;

}
.list-item{

}
.group .groupBox .proList .list-item .leftCont {
  width: 36%;
}
.group .groupBox .proList .list-item .leftCont image {
  width: 220rpx;
  height: 220rpx;
}
.sale_null {
  width: 110rpx;
  height: 110rpx;
  background: rgba(0, 0, 0, 0.3);
  bottom: 42rpx;
  left: 40rpx;
  font-family: 'PingFang-Medium';
}
.qh_time {
  width: 180rpx;
  background: rgba(0, 0, 0, 0.3);
  bottom: 0;
  font-family: 'PingFang-Medium';
}
/* .leftCont{
  overflow:hidden;
  width: 110rpx;
  height: 110px;
} */
/* 商品列表的左侧图片 */
.goodItemImg{
  overflow: hidden;
  width: 180rpx;
  height: 180rpx;
  top: -20rpx;
  left: -40rpx;
  box-shadow: 0rpx 6rpx 16rpx 0rpx rgba(110,110,110,0.1);

}
.goodItemImg image{
  width: 100%;
  height: 100%;
}
.listItem{
  width: 650rpx;
  height: 180rpx;
  margin-top: 30rpx; 
  border-radius: 8rpx;
  
}
.group .groupBox .proList .list-item .rightCont {
  width: 440rpx;
  font-family: 'PingFang-Medium';
}
/* 列表项的文字部分 */
.itemText{
  font-family: 'PingFang-Medium';
  width: 470rpx;
  right: 20rpx;
  height: 180rpx;
}
.title{
  width: 470rpx;
}
.group .groupBox .proList .list-item .rightCont .title text {
  vertical-align: middle;
}
.group .groupBox .proList .list-item .rightCont .title :last-child {
  display: inline;
}
.dian {
  margin-right: 3rpx;
  width: 10rpx;
  height: 10rpx;
  background: #ccc;
}
.dian1{
  /* display: block; */
  width: 16rpx;
  display: flex;
  justify-content: center;

}
.group .groupBox .proList .list-item .rightCont .end_time text.times {
  padding:0 5rpx;
}
.group .groupBox .proList .list-item .rightCont .end_time text.dian {
  margin: 0 5rpx;
}
.group .groupBox .proList .list-item .rightCont .price {
  font-family: 'PingFang-Bold';
}
/* 限量特价加入购物车小图标 */
.addCarIcon{
  margin-left: 165rpx;
  width: 50rpx;
  height: 50rpx;
  bottom:20rpx;
  right:30rpx;
}
.group .groupBox .proList .list-item .rightCont .price .joinCart {
  right: 40rpx;
  bottom: 60rpx;
  width: 39rpx;
  height: 39rpx;
}
.joinGroup {
  border: 2rpx solid #FC5920;
  border-radius: 20rpx;
}
.joinGroup2 {
  border: 2rpx solid #999;
  border-radius: 20rpx;
}
.group .groupBox .loadMore {
  padding: 30rpx 0 20rpx 0;
}
.group .groupBox .loadMore image {
  width: 120rpx;
  height:80rpx;
  margin: 0 auto;
}
.good_box {
  width: 60rpx;
  height: 60rpx;
  position: fixed;
  border-radius: 50%;
  overflow: hidden;
  left: 50%;
  top: 50%;
  z-index: 99;
  border: 1px solid rgba(187, 49, 40, 0.61);
  background: rgba(230, 15, 15, 0.966);
}
.good_box image {
  display: block;
  width: 100%;
  height: 100%;
}

