/* pages/mySubpackage/pages/wallet/wallet.wxss */
.wallet .myBalance {
  padding: 30rpx 30rpx 40rpx 30rpx;
}
.wallet .myBalance .headCont .leftCont,
.wallet .myBalance .headCont .rightCont {
  width: 50%;
}
.wallet .myBalance .headCont .leftCont image {
  width: 35rpx;
  height: 35rpx;
}
.wallet .myBalance .contentBox {
  width: 100%;
  height: 316rpx;
  background: url("data:image/png;base64,");
  background-size: cover;
}
.wallet .myBalance .contentBox .title {
  padding-top: 80rpx;
}
.wallet .myBalance .contentBox .title image {
  width: 30rpx;
  height: 30rpx;
}
.wallet .myBalance .contentBox .money {
  margin-top: 65rpx;
  font-size: 60rpx;
}
.wallet .recharge {
  padding: 0 30rpx 50rpx 30rpx;
}
.wallet .recharge .headCont .leftCont image {
  width: 35rpx;
  height: 35rpx;
}
.wallet .recharge .contentBox .list .list-item {
  width: 100%;
  height: 110rpx;
  background: #eeeeee;
}
.wallet .recharge .contentBox .list .list-item .leftCont {
  margin-right: 70rpx;
}
.wallet .recharge .contentBox .leftCont image {
  width: 180rpx;
  height: 86rpx;
}
.wallet .recharge .contentBox .list .list-item .leftCont text {
  top: 15rpx;
}
.wallet .recharge .contentBox .chooseMoney .content {
  width: 100%;
  height: 110rpx;
  background: #eeeeee;
}
.wallet .recharge .contentBox .chooseMoney .content .leftCont {
  margin-right: 70rpx;
}
.wallet .recharge .contentBox .chooseMoney .content .leftCont image {
  width: 180rpx;
  height: 86rpx;
}
.wallet .recharge .contentBox .chooseMoney .content .leftCont text {
  top: 15rpx;
}
.rightCont image {
  width: 35rpx;
  height: 21rpx;
}
.wallet .recharge .contentBox .otherMoneyBox {
  width: 100%;
  height: 139rpx;
  background: url("data:image/png;base64,");
  background-size: cover;
}
.wallet .recharge .contentBox .otherMoneyBox image {
  width: 100%;
  height: 139rpx;
  z-index: -99;
}
.wallet .recharge .contentBox .otherMoneyBox .inputBox {
  padding: 0 20rpx;
  padding-top: 30rpx;
  width: 100%;
  height: 139rpx;
}
.wallet .recharge .contentBox .otherMoneyBox .inputBox input {
  width: 70%;
  border: 1rpx solid #cccccc;
}
.wallet .recharge .contentBox .otherMoneyBox .inputBox view {
  width: 30%;
}
.explain {
   position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.6);
    z-index: 1010;	
}
.wallet .modalContent {
  position: absolute;
  top: 45%;
  left: 50%;
  transform: translate(-50%,-50%);
  padding: 40rpx;
  width: 80%;
  height: 406rpx;
}
.wallet .modalContent .modalTop image {
  width: 28rpx;
  height: 28rpx;
}
.wallet .modalContent .modalMiddle {
  text-align: left!important;
}
.wallet .modalContent .modalMiddle view {
  line-height: 40rpx;
  margin-top: 15rpx;
}
.wallet .modalContent .modalMiddle view:nth-child(1) {
  margin-top: 0;
}
.wallet .payList {
  position: fixed;
  bottom: 0;
  width: 100%;
}
.wallet .payList .head {
  font-family: 'PingFang-Medium';
}
.wallet .payList .head image {
  margin-top: 36rpx;
  width: 28rpx;
  height: 28rpx;
}
.wallet .payList .list .list-item .leftCont image {
  width: 60rpx;
  height: 60rpx;
}
.wallet .payList .list .list-item .rightCont image {
  margin-top: 32rpx;
  width: 36rpx;
  height: 25rpx;
}
.wallet .payList .list .list-item:last-child {
  padding-bottom: 40rpx;
  border-bottom: none;
}
.wallet .payList .payment {
  font-family: 'PingFang-Medium';
}
.wallet .closeModal {
  position: absolute;
  top: 64%;
  left: 50%;
  transform: translate(-50%,-50%);
  background: #fff !important;
  border-radius: 50%;
  margin: 0 auto;
  margin-top: 40rpx;
  width: 70rpx;
  height: 70rpx;
  display:flex;
  align-items:center;
}
.closeModal image {
  width: 30rpx;
  height: 30rpx;
  margin: 0 auto;
  line-height:70rpx;
}
.wallet .activeColor {
  color: #FC5920;
}
.wallet .activeColor2 {
  background: #FDE5BA !important;
}
.wallet .activeColor2 .rightCont {
  color: #FC5920;
}

