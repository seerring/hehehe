/* pages/mySubpackage/pages/feedback/feedback.wxss */
.myFeedback {
     padding:0 30rpx 30rpx 30rpx;
}
.myFeedback .list-item{
     width:150rpx;
     border:1rpx solid #ccc;
     box-sizing:border-box;
     line-height:38rpx;
}
.myFeedback .list-item:nth-child(4n){
     margin-right:0;
}
.textarea{
     width:690rpx;
}
.textarea textarea{
     padding:20rpx 20rpx 50rpx 20rpx;
     width:690rpx;
     height:157rpx;
     border:1rpx solid #ccc;
     box-sizing:border-box;
}
.textarea .length{
     bottom:10rpx;
     right:10rpx;
}
.lists{
     height:130rpx;
}
.lists image{
     width:130rpx;
     height:130rpx;
}
.imgList .upload{
     width:130rpx;
     height:130rpx;
     border:1rpx dashed #ccc;
}
.imgList .uploadImg{
     width:42rpx;
     height:32rpx;
}
.lists .delete {
     width:24rpx;
     height:24rpx;
     right:-20rpx;
     top:-15rpx;
}
.contactWay{
     padding:20rpx 30rpx;
}
.submitBtn{
     width:690rpx;
}
.modalBox{
     position:fixed;
     left:83rpx;
     top:35%;
     width:583rpx;
     height:332rpx;
}
.modalBox .content{
     margin-top:84rpx;
}
.modalBox .btn{
     margin-top:73rpx;
     height:94rpx;
     line-height:94rpx;
     color:#51a838;
     border-top:2rpx solid #eee;
}
.active{
     color:#fff;
     background:#FBA981;
     border:none!important;
}